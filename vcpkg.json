{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg-tool/main/docs/vcpkg.schema.json",
  "name": "dosbox-staging",
  "description": "DOSBox Staging is a modern continuation of DOSBox with advanced features and current development practices.",
  "version": "0.83.0-alpha",
  "dependencies": [
    "gtest",
    "iir1",
    "libmt32emu",
    "libpng",
    "opusfile",
    "fluidsynth",
    {
      "name": "sdl2",
      "platform": "linux",
      "features": ["alsa", "dbus", "ibus", "wayland", "x11"]
    }, {
      "name": "sdl2",
      "platform": "!linux"
    },
    "sdl2-image",
    "sdl2-net",
    "speexdsp",
    {
      "name": "tracy",
      "platform": "!(windows & arm64)"
    },
    "zlib-ng",
    {
      "name": "pkgconf",
      "host": true
    }
  ],
  "builtin-baseline": "b83220a8325549a2acc026a698a2b23a5a90f403"
}
