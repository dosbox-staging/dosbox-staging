  [drive]
  type    = dir, overlay, floppy lub cdrom
  label   = etykieta_dysku
  path    = ustawienia sciezki wyszukiwania, np. path = %%path%%;c:\tools
  override_drive = nadpisanie litery dysku (domyslnie puste)
